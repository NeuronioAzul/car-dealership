{
    "name": "car-dealership/saga-orchestrator",
    "description": "SAGA Orchestrator for Car Dealership Transactions",
    "type": "project",
    "require": {
        "php": "^8.4",
        "firebase/php-jwt": "^6.8",
        "vlucas/phpdotenv": "^5.5",
        "ramsey/uuid": "^4.7",
        "php-amqplib/php-amqplib": "^3.5"
    },
    "require-dev": {
        "phpunit/phpunit": "^10.0"
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/",
            "Tests\\": "tests/"
        }
    },
    "config": {
        "optimize-autoloader": true
    }
}

